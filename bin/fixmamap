xmodmap -e "keycode 202 = F24"
xmodmap -e "keycode 57 = n N n N less"
xmodmap -e "keycode 58 = m M m M greater"
xmodmap -e "keycode 43 = h H h H braceleft"
xmodmap -e "keycode 44 = j J j J braceright"
xmodmap -e "keycode 29 = z Z z Z bracketleft"
xmodmap -e "keycode 30 = u U u U bracketright"
xmodmap -e "keycode 30 = u U u U bracketright"
