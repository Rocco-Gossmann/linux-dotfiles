xmodmap -e "keycode 15 = 6 ampersand       6 ampersand      F6"
xmodmap -e "keycode 16 = 7 slash           7 slash          F7"
xmodmap -e "keycode 17 = 8 parenleft       8 parenleft      F8"
xmodmap -e "keycode 18 = 9 parenright      9 parenright     F9"
xmodmap -e "keycode 19 = 0 equal           0 equal          F10"
xmodmap -e "keycode 20 = ssharp question   ssharp question  F11"
xmodmap -e "keycode 21 = acute grave       acute grave      F12"